package org.ncapsdev.curtcraft.items;

import cpw.mods.fml.common.registry.GameRegistry;
import net.minecraft.creativetab.CreativeTabs;
import net.minecraft.item.Item;

public class CurtEgg {
	public static void mainRegistry() {
		
	}
	
			public static Item curt_egg;
			

			public static void initializeItem() {
				
				 curt_egg = new GlistreEntityMonsterPlacer("Glistre_Wolf", 0x000000, 0xFFFFFF)
						.setUnlocalizedName ("spawn_egg_")
						.setTextureName("glistremod.spawn_egg");
						

			}

			public static void registerItem() {
				GameRegistry.registerItem(curt_egg, "curtegg");
				
			}
}
